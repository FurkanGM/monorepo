{
  "name": "@kampus/layout-tree",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build:tsup": "tsup src/index.ts --format esm,cjs",
    "build:dts": "tsc --emitDeclarationOnly --declaration",
    "build": "npm run build:tsup && npm run build:dts",
    "clean": "rm -rf dist",
    "dev": "npm run build:tsup -- --watch",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "lint:fix": "TIMING=1 npm run lint -- --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@kampus/eslint-config": "*",
    "@kampus/jest-presets": "*",
    "@kampus/tsconfig": "*",
    "@types/eslint": "^8.4.10",
    "@types/jest": "^29.2.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
